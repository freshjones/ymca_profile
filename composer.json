{
    "name": "freshjones/ymca_profile",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/core": "~8.0",
        "drupal/features": "3.5",
        "drupal/config_update": "1.3",
        "drupal/entity": "1.0-alpha4",
        "drupal/pathauto": "1.0",
        "drupal/ctools": "3.0",
        "drupal/token": "1.0",
        "drupal/migrate_plus": "4.x-dev",
        "drupal/migrate_tools": "4.x-dev",
        "drupal/admin_toolbar": "1.19"
    },
    "require-dev": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "extra": {
        "installer-paths": {
            "public/core": ["type:drupal-core"],
            "public/libraries/{$name}": ["type:drupal-library"],
            "public/modules/contrib/{$name}": ["type:drupal-module"],
            "public/profiles/contrib/{$name}": ["type:drupal-profile"],
            "public/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
